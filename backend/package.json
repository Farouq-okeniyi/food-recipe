{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend food application",
  "main": "./src/index.ts",
  "type": "module",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2020",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "skipLibCheck": true
  },
  "ts-node": {
    "esm": true
  },
  "scripts": {
    "start": "nodemon --watch ./src --exec node --loader ts-node/esm ./src/server.ts"
  },
  "author": "Farouq Okeniyi Tobiloba",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "init": "^0.1.2",
    "morgan": "^1.10.0",
    "prima": "^0.0.1",
    "prisma": "^6.0.1",
    "undici": "^7.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
